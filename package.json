{
  "name": "holyjs-examples",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "stylelint": "stylelint \"**/*.css\"",
    "pretty": "yarn format && yarn lint --fix && yarn stylelint --fix",
